        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Margin="203,270,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="62" Height="16"/>
        <TextBlock x:Name="textBoxDescription1" HorizontalAlignment="Left" Margin="474,22,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="26" Width="245" Background="#FF75FFAA" TextAlignment="Center" LineHeight="7" FontWeight="Bold"/>
        <TextBlock x:Name="textBoxDescription2" HorizontalAlignment="Left" Margin="474,73,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="26" Width="245" Background="#FF75FFAA" TextAlignment="Center" LineHeight="7" FontWeight="Bold"/>
        <TextBlock x:Name="textBoxDescription3" HorizontalAlignment="Left" Margin="474,125,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="26" Width="245" Background="#FF75FFAA" TextAlignment="Center" LineHeight="7" FontWeight="Bold"/>
        <TextBlock x:Name="textBoxDescription4" HorizontalAlignment="Left" Margin="474,184,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="26" Width="245" Background="#FF75FFAA" TextAlignment="Center" LineHeight="7" FontWeight="Bold"/>
        <TextBlock x:Name="textBoxDescription5" HorizontalAlignment="Left" Margin="474,231,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="26" Width="245" Background="#FF75FFAA" TextAlignment="Center" LineHeight="7" FontWeight="Bold"/>
        <TextBlock x:Name="textBoxDescription6" HorizontalAlignment="Left" Margin="474,290,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="26" Width="245" Background="#FF75FFAA" TextAlignment="Center" LineHeight="7" FontWeight="Bold"/>
        <TextBlock x:Name="textBoxDescription7" HorizontalAlignment="Left" Margin="474,342,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="26" Width="245" Background="#FF75FFAA" TextAlignment="Center" LineHeight="7" FontWeight="Bold"/>
        <Button x:Name="buttonContinue" Content="Continue" HorizontalAlignment="Left" Margin="60,370,0,0" VerticalAlignment="Top" Height="39" Width="60" Click="onClickButtonContinue"/>
        <Button x:Name="buttonBack" Content="Back" HorizontalAlignment="Left" Margin="50,20,0,0" VerticalAlignment="Top" Width="40" Click="onButtonBackClick"/>
        <Label x:Name="pointsLb" Content="Score : 0" HorizontalAlignment="Left" Margin="327,40,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
